{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:rollup": "node_modules/.bin/rollup -c ./rollup-config.js",
    "build:aot1": "ngc -p ./src/tsconfig-aot.json && rollup -c ./rollup-config.js",
    "build:aot": "ngc -p ./src/tsconfig-aot.json && rollup -c ./rollup-config.js && node ./copy-dist-files.js",
    "build:sass": "node-sass -r src/ -o src/",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c=bs-config-aot.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "buildandserve:aot": "npm run build:aot && npm run serve:aot",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\" \"npm run watch:sass\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "watch:sass": "npm run build:sass && node-sass src/ -wo src --source-map true",
    "watch:iis": "node ./copy-iis-files.js && concurrently \"npm run build:watch\" \"npm run watch:sass\"",
    "watch:jit": "node ./copy-jit-files.js && concurrently \"npm run build:watch\" \"npm run serve\" \"npm run watch:sass\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.1.3",
    "@angular/compiler": "~4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "~4.1.3",
    "@angular/forms": "~4.1.3",
    "@angular/http": "~4.1.3",
    "@angular/platform-browser": "~4.1.3",
    "@angular/platform-browser-dynamic": "~4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@angular/router": "~4.1.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular-eonasdan-datetimepicker": "^0.3.9",
    "angular-in-memory-web-api": "~0.3.2",
    "bootstrap": "^4.0.0-alpha.6",
    "clarity-angular": "^0.9.5-patch",
    "clarity-icons": "^0.9.5-patch",
    "clarity-ui": "^0.9.5-patch",
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ngx-bootstrap": "^1.6.6",
    "ngx-pagination": "^3.0.0",
    "node-sass": "^4.5.3",
    "rxjs": "5.4.0",
    "sass-loader": "^6.0.5",
    "systemjs": "0.20.12",
    "tether": "^1.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.3.1",
    "@types/faker": "^4.1.0",
    "@types/jasmine": "2.5.47",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.22",
    "bootstrap": "^3.3.7",
    "browserify": "^14.3.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.4.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "jasmine-core": "~2.6.2",
    "jquery": "^3.2.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "source-map-explorer": "^1.3.3",
    "stacktrace-js": "^2.0.0",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3",
    "webpack": "^2.6.0"
  },
  "repository": {}
}
